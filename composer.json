{
  "name": "sami/feed-data-processor",
  "description": "XML data processor for feeding into MySQL database",
  "require": {
    "php": "^7.3.29",
    "monolog/monolog": "^2.3",
    "phpunit/phpunit": "^8.2",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "vlucas/phpdotenv": "^5.6",
    "ext-json": "*",
    "ext-xmlreader": "*"
  }
  ,
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Config\\": "config/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.3.29"
    }
  }
}
